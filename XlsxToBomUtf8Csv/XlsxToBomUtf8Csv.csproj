<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <PublishAot>true</PublishAot>
        <InvariantGlobalization>true</InvariantGlobalization>
        <Copyright>flier268</Copyright>
        <PackageProjectUrl>https://github.com/flier268/CsvConverter</PackageProjectUrl>
        <PackageLicenseUrl>https://raw.githubusercontent.com/flier268/CsvConverter/main/LICENSE</PackageLicenseUrl>
        <Authors>flier268</Authors>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="MiniExcel" Version="1.32.1" />
    </ItemGroup>

</Project>
